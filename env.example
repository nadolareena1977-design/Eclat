# Razorpay Configuration
# Copy this file to .env and fill in your actual values

# Test Environment (for development)
RAZORPAY_TEST_KEY_ID=rzp_test_your_test_key_id_here
RAZORPAY_TEST_KEY_SECRET=your_test_key_secret_here

# Production Environment (for live transactions)
RAZORPAY_PRODUCTION_KEY_ID=rzp_live_your_production_key_id_here
RAZORPAY_PRODUCTION_KEY_SECRET=your_production_key_secret_here

# Node Environment (set to 'production' for live environment)
NODE_ENV=development
